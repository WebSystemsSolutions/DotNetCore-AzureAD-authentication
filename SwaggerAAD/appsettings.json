{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "https://login.microsoftonline.com/{tenantId}/oauth2/authorize",
    "TenantId": "{tenantId}",
    "ClientId": "{clientId}",
    "CallbackPath": "/"
  },
  "Swagger": {
    "ClientId": "{clientId}",
    "ClientSecret": "{clientSecret}"
  },
  "AllowedHosts": "*"
}
